{
    "name": "tohuwabohu-confluence",
    "version": "1.0.0",
    "author": "Martin Meinhold",
    "license": "Apache-2.0",
    "summary": "Puppet module for Atlassian Confluence",
    "source": "https://github.com/tohuwabohu/puppet-confluence.git",
    "project_page": "https://github.com/tohuwabohu/puppet-confluence",
    "issues_url": "https://github.com/tohuwabohu/puppet-confluence/issues",
    "tags": ["confluence", "wiki", "atlassian"],
    "operatingsystem_support": [
        {
            "operatingsystem": "Debian",
            "operatingsystemrelease": [ "7.0" ]
        }
    ],
    "requirements": [
        {
            "name": "puppet",
            "version_requirement": "3.x"
        }
    ],
    "dependencies": [
        {
            "name": "camptocamp/archive",
            "version_requirement": "0.x"
        },
        {
            "name": "puppetlabs/stdlib",
            "version_requirement": "4.x"
        }
    ]
}
